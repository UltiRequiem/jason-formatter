{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "Node",
    "target": "ES2020",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "importHelpers": false,
    "skipLibCheck": true,
    "outDir": "lib",
    "declaration": true
  },
  "include": ["src/**/*"]
}
